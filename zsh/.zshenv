# Default editor
export EDITOR='nvim'

# Workspace directory
export WORKSPACE=$HOME/code

# nvm root
export NVM_DIR="$HOME/.nvm"

# git language
export LANG=en_US.UTF-8

# Add sbin to path
export PATH="/usr/local/sbin:$PATH"

# add local bin
export PATH="$HOME/.local/bin:$PATH"

export DOTFILES=$HOME/.dotfiles
